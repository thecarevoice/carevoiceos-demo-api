# Server Configuration
PORT=3000
NODE_ENV=development

# CareVoiceOS OpenAPI Configuration
CAREVOICE_API_BASE_URL=<replace_me>
CAREVOICE_API_KEY=<replace_me>
CAREVOICE_CLIENT_ID=<replace_me>
CAREVOICE_CLIENT_SECRET=<replace_me>
CAREVOICE_GROUP=<replace_me>

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRES_IN=24h

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS Configuration
CORS_ORIGIN=http://localhost:3000,http://localhost:8081 